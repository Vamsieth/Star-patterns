#include <stdio.h>
 
int main () {
	/* a program to print star #1
	
	*                    ( No.of row == coloums)
	**       
	***
	**** 
	*/
	
	int i, j,n;
	printf("Enter a number:\n");
	scanf("%d", &n);
	
	for (i = 1; i<= n; i++)
	{
		for (j = 1; j<= i; j++ )
		{
			printf("*");
		}
	   printf("\n");
	}
   return 0;
}
